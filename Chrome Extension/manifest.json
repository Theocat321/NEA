{
    "manifest_version": 3,
    "name": "Twitter Senti Viz",
    "version": "1.0",
    "author": "Adam O'Neill",
    "description": "Sentiment analysis tool for your twitter timeline",
    "permissions": [
      "webRequest",
      "http://127.0.0.1:8000/"
    ],
    "content_scripts": [
      {
        "matches": ["*://twitter.com/*"],
        "js": ["main.js"],
        "css":["main.css"]
        
      }
    ]        
}