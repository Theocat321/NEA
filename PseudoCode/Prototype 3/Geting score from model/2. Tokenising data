# Tokenizing data (JS)

function tokenizeText(text){
    tokenizedText = ""
    split text into words
    For everyWord in words:
        everyWord = WordEmbeddings[everyWord]
        if undefined:
            skip
        tokenizedText += everyWord
    return tokenizedText
}

constant WordEmbeddings = {......} //Dictionary of all words and corespodning numbers


